  "author": "",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^118.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vercel/node": "^3.0.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vercel": "^33.0.1"
  }
}